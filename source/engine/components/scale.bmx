'------------------------------------------------------------------------------
' Component: TScale
'------------------------------------------------------------------------------
Type TScale


'------------------------------------------------------------------------------
' Properties of TScale
'------------------------------------------------------------------------------
	Field sx:Float = 1.0
	Field sy:Float = 1.0


'------------------------------------------------------------------------------
' Setter
' Info: Range Checking only in Debug Mode
'------------------------------------------------------------------------------
	Method Set( scale_x:Float, scale_y:Float )
		sx = scale_x
		sy = scale_y
	EndMethod
	
		
EndType
